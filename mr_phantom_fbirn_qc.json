{
  "submission": {
    "data": {
      "zIndex": 10,
      "xnatDatatype": {
        "label": "MR Session",
        "value": "xnat:mrSessionData"
      },
      "isThisASiteWideConfiguration": "yes"
    }
  },
  "builder": {
    "display": "form",
    "title": "MR Phantom QC (fBIRN)",
    "settings": {},
    "components": [
      {
        "type": "datetime",
        "label": "Scan Date",
        "key": "scanDate",
        "format": "yyyy-MM-dd",
        "enableTime": false,
        "input": true,
        "tableView": true
      },
      {
        "type": "number",
        "label": "SNR",
        "key": "snr",
        "input": true,
        "tableView": true,
        "validate": {
          "min": 0
        }
      },
      {
        "type": "number",
        "label": "Drift (%)",
        "key": "drift",
        "input": true,
        "tableView": true,
        "validate": {
          "min": 0,
          "max": 100
        },
        "step": 0.01
      },
      {
        "type": "number",
        "label": "Ghosting (%)",
        "key": "ghost",
        "input": true,
        "tableView": true,
        "validate": {
          "min": 0,
          "max": 100
        },
        "step": 0.01
      },
      {
        "type": "number",
        "label": "Head Motion (mm)",
        "key": "motion",
        "input": true,
        "tableView": true,
        "validate": {
          "min": 0
        }
      },
      {
        "type": "select",
        "label": "Overall QC",
        "key": "overallQc",
        "input": true,
        "tableView": true,
        "dataSrc": "values",
        "data": {
          "values": [
            {
              "label": "Pass",
              "value": "pass"
            },
            {
              "label": "Conditional",
              "value": "conditional"
            },
            {
              "label": "Fail",
              "value": "fail"
            }
          ]
        },
        "validate": {
          "required": true
        }
      },
      {
        "type": "textarea",
        "label": "Comments",
        "key": "comments",
        "input": true,
        "tableView": true
      }
    ]
  }
}